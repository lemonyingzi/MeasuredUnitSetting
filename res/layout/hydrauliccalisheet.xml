<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:gravity="center">
    <!-- conclusion-->
    <TextView
        android:id="@+id/conclusionTv"
        android:layout_width="0dp"
        android:layout_height="30dp"
		android:layout_weight="1"
		android:gravity="center"
		android:textSize="10sp">
    </TextView>
    <!-- marginError-->
	<TextView
	    android:id="@+id/marginErrorTv"
	    android:layout_width="0dp"
	    android:layout_height="30dp"
	    android:layout_weight="1"
	    android:gravity="center"
	    android:textSize="10sp">
	</TextView>
	<!-- outputValue -->
	<TextView
	    android:id="@+id/outputValueTv"
	    android:layout_width="0dp"
	    android:layout_height="30dp"
	    android:layout_weight="1"
	    android:gravity="center"
	    android:textSize="10sp">
	</TextView>
	<!-- measurePoint -->
	<TextView
	    android:id="@+id/measurePointTv"
	    android:layout_width="0dp"
	    android:layout_height="30dp"
	    android:layout_weight="1"
	    android:gravity="center"
	    android:textSize="10sp">
	</TextView>
	<!-- temperature -->
	<TextView
	    android:id="@+id/temperatureTv"
	    android:layout_width="0dp"
	    android:layout_height="30dp"
	    android:layout_weight="1"
	    android:gravity="center"
	    android:textSize="10sp">
	</TextView>
	<!-- error -->
	<TextView
	    android:id="@+id/errorTv"
	    android:layout_width="0dp"
	    android:layout_height="30dp"
	    android:layout_weight="1"
	    android:gravity="center"
	    android:textSize="10sp">
	</TextView>
	<!-- measuredValue -->
	<TextView
	    android:id="@+id/measuredValueTv"
	    android:layout_width="0dp"
	    android:layout_height="30dp"
	    android:layout_weight="1"
	    android:gravity="center"
	    android:textSize="10sp">
	</TextView>
	<!-- coeff -->
	<TextView
	    android:id="@+id/coeffTv"
	    android:layout_width="0dp"
	    android:layout_height="30dp"
	    android:layout_weight="1"
	    android:gravity="center"
	    android:textSize="10sp">
	</TextView>
</LinearLayout>